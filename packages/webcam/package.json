{
  "name": "ts-webcam",
  "version": "0.1.0",
  "private": false,
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "lib/**"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --outDir lib",
